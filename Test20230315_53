import java.util.Scanner;
import java.util.Random;//Random：生成伪随机数
public class Test53{
	public static void main(String[] args){
		int[] nums = new int[5];
		int len = nums.length;
		Random r = new Random();//创建一个可以生成随机数的工具
		for(int i = 0;i<len;i++){
		nums[i] = r.nextInt(50);
		}
		Scanner input = new Scanner(System.in);
		System.out.println("请输入你要的数：（50以内）");
		int userNum = input.nextInt();
		boolean flag = false;
		for(int x:nums){
			if(userNum==x){
			flag = true;
				break;
			}				
		}
		if(flag){
			System.out.println("猜对了");
		}else{
			System.out.println("猜错了");
		}
	}
}
