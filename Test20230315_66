import java.util.Scanner;
public class Test66{
	public static void main(String[] args){
		Scanner input = new Scanner(System.in);
		System.out.println("请输入一个数");	
		int n = input.nextInt();
		int i = 1;
		int sum = 0;
		for(;i<=n;i++){
			if(n%i == 0){
				sum+=i;
			}
		}
		System.out.println(sum);
	}
}
