public class Test68{
	public static void main(String[] args){
		
		//必须保证数列是有序的
		int[] num = {10,20,30,50,65,88};
		int index = binarySearch(num,30);
		System.out.println(index);
	}
	//二分查找算法
	public static int binarySearch(int[] num,int key){//在哪找，找什么
		int start = 0;//开始下标
		int end = num.length-1;//结束下标
		
		while(start<=end){
			int middle = (start+end)/2;
			if(num[middle]>key){
				end = middle-1;
			}else if(num[middle]<key){
				start = middle+1;
			}else{
				return middle;
			}
		}
		return -1;
	}
}
//binarySearch对分查找；二进位检索；折半查找法；二分搜寻
//middle中间
//index索引
