public class Test81{
	public static void main(String[] args){
		double y;
		Test81 x = new Test81();
		y = x.yang();
		System.out.println(y);
	}
	public double yang(){
		double n = 10,r = 1,sum = 0,flag = 1,m = 1,sums = 0;
		for(double i = 1;i<=n;i++){
			r*=i;
			sum+=r;
			sums+=m/sum*flag;
			flag*= -1;
		}
		return sums;
	}
}
