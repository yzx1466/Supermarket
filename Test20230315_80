import java.util.Scanner;
public class Test80{
	public static void main(String[] args){
		int[] z;
		Test80 y = new Test80();
		z = y.zheng();
		System.out.println("-------------");
		for(int i:z){
		System.out.print(i+" ");
		}
	}
	public  int[] zheng(){
		Scanner reader = new Scanner(System.in);
		int[] m = new int[8];
		System.out.println("请输入8个整数");
	
		for(int i = 0;i<8;i++){
			System.out.println("第"+(i+1)+"个数是：");
			m[i] = reader.nextInt();
		}
		for(int i = 0;i<m.length-1;i++){
			for(int j = 0;j<m.length-1-i;j++){
				if(m[j]>m[j+1]){
					m[j] = m[j]+m[j+1];
					m[j+1] = m[j]-m[j+1];
					m[j] = m[j]-m[j+1];
				}
			}
		}
		return m;	
	}
}
