package Test;
/**
 * 对象需要具有克隆功能
 * 1、实现Cloneable接口,(标记接口)
 * 2、重写Object类中的clone方法
 * @author yangzixi
 *
 */
public class Test46_cat implements Cloneable{
	private String name;
	private int age;
	
	public Test46_cat() {
		super();
	}
	public Test46_cat(String name, int age) {
		super();
		this.name = name;
		this.age = age;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
	@Override
	public String toString() {
		return "Test46_cat [name=" + name + ", age=" + age + "]";
	}
	//重写Object中的clone方法
	@Override
	protected Object clone() throws CloneNotSupportedException {
		return super.clone();
	}
}

